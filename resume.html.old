<script>
// Hydrate Projects from resume/resume.md (simple parser for the custom list format)
document.addEventListener('DOMContentLoaded', async () => {
  try {
    const res = await fetch('resume/resume.md', { cache: 'no-cache' });
    if (!res.ok) throw new Error('resume.md not found');
    const text = await res.text();
    const lines = text.split(/\r?\n/);
    const projects = [];
    let current = null;
    for (const raw of lines) {
      const line = raw.trim();
      if (!line) continue;
      if (line.startsWith('- title:')) {
        if (current) projects.push(current);
        current = { title: line.replace('- title:', '').trim() };
      } else if (current && line.startsWith('timeframe:')) {
        current.timeframe = line.replace('timeframe:', '').trim();
      } else if (current && line.startsWith('summary:')) {
        current.summary = line.replace('summary:', '').trim();
      } else if (current && line.startsWith('tags:')) {
        const match = line.match(/\[(.*)\]/);
        current.tags = match ? match[1].split(',').map(s => s.trim()) : [];
      } else if (current && line.startsWith('image:')) {
        current.image = line.replace('image:', '').trim();
      }
    }
    if (current) projects.push(current);

    if (projects.length) {
      const container = document.querySelector('.resume-grid');
      if (container) {
        // Replace the last UL under the right column Projects section
        const headings = Array.from(container.querySelectorAll('h2'));
        const projHeading = headings.find(h => h.textContent.trim().toLowerCase() === 'projects');
        if (projHeading) {
          const ul = projHeading.nextElementSibling && projHeading.nextElementSibling.matches('ul.plain')
            ? projHeading.nextElementSibling
            : projHeading.parentElement.querySelector('ul.plain');
          if (ul) {
            ul.innerHTML = projects.map(p => {
              const tags = (p.tags || []).slice(0,3).map(t => `<span class="netflix-tag">${t}</span>`).join(' ');
              return `<li><strong>${p.title}:</strong> ${p.summary} ${tags ? `<br>${tags}` : ''}</li>`;
            }).join('');
          }
        }
      }
    }
  } catch (e) {
    console.warn('Could not hydrate Projects from resume.md:', e.message);
  }
});
</script>
